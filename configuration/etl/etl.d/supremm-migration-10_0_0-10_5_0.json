{
    "module": "supremm",
    "defaults": {
        "global": {
            "endpoints": {
                "destination": {
                    "type": "mysql",
                    "name": "SUPReMM DB",
                    "config": "datawarehouse",
                    "schema": "modw_supremm"
                }
            }
        }
    },
    "migration-10_0_0-10_5_0": [
        {
            "name": "table-create",
